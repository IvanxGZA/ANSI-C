#include<stdio.h>
// print Celsius-Fahrenheit table
int main(){
  float fahr, celsius;
  int upper, lower, step;

  lower = -30; //lower limit of temperature table
  upper = 150; //upper limit
  step = 5;   //step size

  celsius = lower;
  printf("Celsius-Fahrenheit Table\n\n");
  while(celsius <= upper){
    fahr = (celsius * 1.8) + 32.0;
    printf("\t%3.0f %3.0f\n", celsius, fahr);
    celsius = celsius + step;
  }
}
